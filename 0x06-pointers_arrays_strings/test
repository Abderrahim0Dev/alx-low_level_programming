#include "main.h"

char *_strncpy(char *dest, char *src, int n) {

  int i = 0, j = 0;

  while (dest[i] != '\0') {
    i++;
  }

  while (j < n) {
    if (src[j] != '\0')
      dest[j] = src[j];
    else {
      dest[j] = '\0';
      break;
    }
    j++;
    i++;
  }

  dest[i] = '\0';

  return (dest);
}
